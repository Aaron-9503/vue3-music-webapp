{
    "compilerOptions": {
        "experimentalDecorators": true,
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@/service/*": [
                "src/service/*"
            ],
            "@util/*": [
                "src/utils/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}